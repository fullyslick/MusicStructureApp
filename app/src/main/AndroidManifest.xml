<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructureapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/select_genere">
            <intent-filter android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ElectronicaActivity" android:label="@string/electronica_label"/>
        <activity android:name=".DiscoActivity" android:label="@string/disco_label"/>
        <activity android:name=".HipHopActivity" android:label="@string/hip_hop_label" />
        <activity android:name=".RockActivity" android:label="@string/rock_label" />
        <activity android:name=".PopActivity" android:label="@string/pop_label" ></activity>
    </application>

</manifest>